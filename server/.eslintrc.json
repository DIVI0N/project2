{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "semi": "error",
        "quotes": [
            "error",
            "single"
        ],
        "no-console": "warn",
        "no-unused-vars": "warn"
    }
}